pipeline {
    agent any  // Define el agente donde se ejecutarÃ¡ el pipeline. Puedes usar 'any' para cualquier agente disponible.
    stages {
        stage('cypress parallel test') {
            parallel {
                stage('tester A') {
                    steps {
                        sh "npm run cy:run_edge"
                    }
                }
                stage('tester B') {
                    steps {
                        sh "npm run run_chrome"
                    }
                }
            }
        }
    }
}
