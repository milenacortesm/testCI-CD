pipeline {
    agent any // Define el agente donde se ejecutarÃ¡ el pipeline. Puedes usar 'any' para cualquier agente disponible.

    stages {
        stage('cypress parallel test') {
            parallel {
                stage('tester A') {
                    steps {
                        sh "npm run cypress:run"
                    }
                }

                stage('tester B') {
                    agent { label "mac" } // Define un agente especÃ­fico para esta etapa
                    steps {
                        sh "npm run cypress:run"
                    }
                }
            }
        }
    }
}
